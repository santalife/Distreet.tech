                    console.log(pair[0] + ', ' + pair[1]);
                }
                $.ajax({
                    url: "/user/profile/post/photo&video",
                    type: 'POST',
                    contentType: false,
                    processData: false,
                    'success': (data) => {
                        setInterval('location.reload()', 100);
                    }
                })
            })
        })

    </script>